{
  "name": "google-maps-scanner-pro",
  "version": "1.0.0",
  "description": "Chrome Extension chuyên nghiệp để scan và thu thập dữ liệu từ Google Maps - Made for Vietnamese Developers",
  "author": "Vietnamese Python Developers",
  "license": "MIT",
  "keywords": [
    "google-maps",
    "chrome-extension",
    "web-scraping",
    "data-extraction",
    "vietnam",
    "automation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/google-maps-scanner"
  },
  "homepage": "https://github.com/yourusername/google-maps-scanner#readme",
  "bugs": {
    "url": "https://github.com/yourusername/google-maps-scanner/issues"
  },
  "scripts": {
    "create-icons": "python3 create_icons.py",
    "test": "python3 test_extension.py",
    "validate": "python3 -m json.tool manifest.json",
    "zip": "zip -r google-maps-scanner.zip . -x '*.git*' -x 'node_modules/*' -x '*.DS_Store'"
  },
  "devDependencies": {},
  "requirements": {
    "python": [
      "Pillow>=9.0.0",
      "pandas>=1.3.0",
      "requests>=2.26.0"
    ],
    "browser": [
      "Google Chrome >= 88",
      "Microsoft Edge >= 88"
    ]
  },
  "features": [
    "Auto-scroll scanning",
    "Smart data extraction",
    "CSV/JSON export",
    "Chrome Storage integration",
    "Real-time progress tracking",
    "Duplicate detection",
    "Vietnamese UI",
    "Customizable settings"
  ],
  "languages": {
    "JavaScript": "85%",
    "HTML": "10%",
    "CSS": "5%"
  }
}
